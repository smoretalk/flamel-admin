{
  "name": "flamel-admin",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./customProperty": {
      "types": "./customProperty.d.ts",
      "default": "./customProperty.js"
    },
    "./customResource": {
      "types": "./customResource.d.ts",
      "default": "./customResource.js"
    },
    "./customRecord": {
      "types": "./customRecord.d.ts",
      "default": "./customRecord.js"
    }
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "version": "0.0.1",
  "dependencies": {
    "@adminjs/design-system": "^4.1.1",
    "@adminjs/prisma": "^5.0.3",
    "@mediapipe/tasks-text": "^0.10.20",
    "@mediapipe/tasks-vision": "^0.10.20",
    "@microlink/react-json-view": "github:zerocho/react-json-view",
    "@prisma/client": "^6.1.0",
    "adminjs": "^7.8.13",
    "auto-palette": "^1.3.3",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "qrious": "^4.0.2",
    "qs": "^6.13.1",
    "react": "^19.0.0",
    "react-diff-viewer-continued": "^3.4.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-router": "^7.0.2",
    "react-router-dom": "^7.0.2",
    "react-select": "^5.9.0",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/flat": "^5.0.5",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20.17.10",
    "@types/qs": "^6.9.17",
    "@types/react": "^19.0.2",
    "@types/styled-components": "^5.1.34",
    "jest": "^29.7.0",
    "patch-package": "^8.0.0",
    "ts-jest": "^29.2.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "@adminjs/prisma/(.*)": [
        "<rootDir>/node_modules/@adminjs/prisma/$1"
      ]
    },
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.js$",
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
